{
  "AWSEBDockerrunVersion": 2,
  "volumes": [],
  "containerDefinitions": [
    {
      "name": "anime-lineup",
      "image": "quay.io/tsukaby/anime-lineup",
      "essential": true,
      "memory": 1024
    },
    {
      "name": "c-antenna",
      "image": "quay.io/tsukaby/c-antenna",
      "essential": true,
      "memory": 2048
    },
    {
      "name": "nginx-proxy",
      "image": "quay.io/tsukaby/tsukaby-apps-proxy",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "anime-lineup",
        "c-antenna"
      ],
      "mountPoints": []
    }
  ]
}